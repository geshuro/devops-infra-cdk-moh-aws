{
  "name": "@aws-ee/ui",
  "version": "0.1.0",
  "private": true,
  "description": "Client-side UI application",
  "author": "Amazon Web Services",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "/",
  "main": "build/index.html",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@aws-ee/core": "workspace:*",
    "@aws-ee/core-ui": "workspace:*",
    "@chakra-ui/react": "^1.6.9",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fast-csv/parse": "^4.3.6",
    "@hookform/resolvers": "^2.8.1",
    "animate.css": "3.7.0",
    "aws-sdk": "^2.713.0",
    "class-validator": "^0.13.1",
    "classnames": "^2.2.6",
    "framer-motion": "^4",
    "lodash": "^4.17.15",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "mobx-state-tree": "^5.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-avatar": "^3.9.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.16.1",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-table": "^6.11.4",
    "roddeh-i18n": "^1.2.0",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "@aws-ee/eslint-config": "workspace:*",
    "@craco/craco": "^6.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "craco-esbuild": "^0.4.1",
    "eslint": "^7.32.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "prettier": "^2.3.1",
    "react-scripts": "4.0.3",
    "typescript": "~4.4.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "craco start",
    "bundle": "INLINE_RUNTIME_CHUNK=false craco build",
    "test": "craco test --watchAll=false --passWithNoTests",
    "test:watch": "craco test --passWithNoTests",
    "eject": "craco eject",
    "lint": "pnpm run compile && pnpm run lint:eslint && pnpm run lint:prettier",
    "lint:eslint": "eslint --ignore-path .gitignore . ",
    "lint:prettier": "prettier --check --ignore-path .gitignore 'src/**/*.{ts,tsx}' ",
    "format": "pnpm run format:eslint && pnpm run format:prettier",
    "format:eslint": "eslint --fix --ignore-path .gitignore . ",
    "format:prettier": "prettier --write --ignore-path .gitignore 'src/**/*.{ts,tsx}' ",
    "compile": "tsc --project ./tsconfig.json --noEmit",
    "compile:watch": "tsc --project ./tsconfig.json --noEmit --watch"
  }
}

{
  "name": "start-screening",
  "version": "1.0.0",
  "description": "## Config",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node --transpile-only --compiler typescript-cached-transpile src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "pnpm run lint:eslint && pnpm run lint:prettier",
    "lint:eslint": "eslint --quiet --ignore-path .gitignore 'lib/**/*.{ts,tsx}'",
    "lint:prettier": "prettier --check --ignore-path .gitignore 'lib/**/*.{ts,tsx}'",
    "format": "pnpm run format:eslint && pnpm run format:prettier",
    "format:eslint": "eslint --fix --ignore-path .gitignore 'lib/**/*.{ts,tsx}'",
    "format:prettier": "prettier --write --ignore-path .gitignore 'lib/**/*.{ts,tsx}'",
    "compile": "tsc --project ./tsconfig.json --noEmit",
    "compile:watch": "tsc --project ./tsconfig.json --noEmit --watch"
  },
  "author": "",
  "devDependencies": {
    "@aws-cdk/assert": "1.123.0",
    "@aws-ee/eslint-config": "workspace:*",
    "@aws-ee/backend-screening-input": "workspace:*",
    "@aws-ee/backend-common": "workspace:*",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "1.123.0",
    "eslint": "^7.21.0",
    "jest": "^26.4.2",
    "prettier": "^2.3.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~4.4.2"
  },
  "dependencies": {
    "@nestjs/common": "^8.1.1",
    "@aws-ee/core": "workspace:*",
    "@aws-sdk/client-s3": "^3.36.1"
  },
